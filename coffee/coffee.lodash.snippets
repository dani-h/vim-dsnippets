# -----------------------------------------------------
# Objects
# -----------------------------------------------------

snippet values "_.values(obj)"
_.values(${1:arg})
endsnippet

snippet clone "_.clone(obj) (shallow)"
_.clone(${1:arg})
endsnippet

snippet clonedeep "_.cloneDeep(obj) (deep)"
_.cloneDeep(${1:arg})
endsnippet


# -----------------------------------------------------
# Chainable
# -----------------------------------------------------

snippet value "_.value() (chain value)" w
value()
endsnippet

# -----------------------------------------------------
# Collections
# Do not add methods that already exist on native arrays
# Use coffee.snippets for that
# -----------------------------------------------------
snippet partition" "partition(predicate)" w
partition((${1:arg}) -> ${2:predicate})
endsnippet

snippet find "find(predicate)" w
find((${1:arg}) -> ${2:predicate})
endsnippet

snippet includes "includes(predicate)" w
includes((${1:arg}) -> ${2:predicate})
endsnippet
# -----------------------------------------------------
# Strings
# -----------------------------------------------------
snippet _.capitalize "_.capitalize(str)"
_.capitalize.(${1:str})
endsnippet

snippet _.endswith "_.endsWith(str, str)"
_.endsWith(${1:str}, ${2:str})
endsnippet

snippet _.startswith "_.startsWith(str, str)"
_.startsWith(${1:str}, ${2:str})
endsnippet
